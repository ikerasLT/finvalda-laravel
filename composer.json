    "license": "MIT",
    "require": {
        "php": "^7.0",
        "illuminate/support": "^5.1",
        "vlucas/phpdotenv": "^2.4",
        "guzzlehttp/guzzle": "^6.3",
        "netresearch/jsonmapper": "^1.2",
        "nesbot/carbon": "^1.22",
        "nathanmac/Parser": "^4.3"
    },
    "require-dev": {
